    <sql>CREATE OR REPLACE VIEW sales AS
	SELECT
  salebillvouchs.sale_bill_id, 
  salebillvouchs.inventory_code, 
  salebillvouchs.quantity, 
  salebillvouchs.unit_gross_sell_price, 
  salebillvouchs.unit_net_sell_price, 
  salebillvouchs.total_gross_sell_price, 
  salebillvouchs.total_net_sell_price, 
  salebillvouchs.signature_date, 
  salebillvouchs.sale_order_code, 
  salebillvouchs.total_tax_price, 
  salebillvouchs.discount_price, 
  salebillvouchs.unit_gross_sell_price_in_local_currency, 
  salebillvouchs.total_gross_sell_price_in_local_currency, 
  salebillvouchs.total_tax_price_in_local_currency, 
  salebillvouchs.total_net_sell_price_in_local_currency,
  salebillvouchs.transfer, 
  salebillvouchs.tax_rate,
  salebillvouchs.delivery_code,
  salebillvouch.currency_change_rate, 
  salebillvouch.currency, 
  salebillvouch.sale_class_code, 
  salebillvouch.customer_code, 
  salebillvouch.person_code, 
  salebillvouch.department_code, 
  salebillvouch.sale_bill_date, 
  salebillvouch.sale_type_code, 
  salebillvouch.vouch_type, 
  salebillvouch.sale_bill_code,
  dispatchlist.delivery_date,
  inventory.inventory_name,
  inventory.inventory_class_code,
  inventoryclass.inventory_class_name,
  crm_customer_list.name as customer_name,
  crm_customer_list.english_name as customer_english_name,
  crm_customer_list.class_name as customer_class_name,
  person.name as person_name
	FROM 
		salebillvouchs
	LEFT JOIN salebillvouch ON salebillvouchs.sale_bill_id = salebillvouch.sale_bill_id
	LEFT JOIN dispatchlist ON salebillvouchs.delivery_code = dispatchlist.delivery_code
    LEFT JOIN inventory ON salebillvouchs.inventory_code = inventory.inventory_code
    LEFT JOIN inventoryclass ON inventory.inventory_class_code = inventoryclass.inventory_class_code
    LEFT JOIN crm_customer_list ON salebillvouch.customer_code=crm_customer_list.code
    LEFT JOIN person ON salebillvouch.person_code=person.code</sql>
