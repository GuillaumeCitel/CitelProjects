        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var bill_id = RNR &#47;&#47;number
var order_nb = OBJNR; &#47;&#47;string
var bill_date = DATUM;&#47;&#47;date
var customer_id = KDNR;&#47;&#47;number
var customer_nb = SORTNR;&#47;&#47;string
var customer_name = SORT;&#47;&#47;string
var product_gmbh_code = ANR;&#47;&#47;string
var product_name = BEZ;&#47;&#47;string
var product_gmbh_group_1 = GRU;&#47;&#47;string
var product_gmbh_group_2 = MWKZ;&#47;&#47;string
var quantity = MENGE;&#47;&#47;number
var unit_buy_price = EK;&#47;&#47;number
var total_buy_price = quantity * unit_buy_price;&#47;&#47;number
var unit_gross_sell_price_without_discount = VK;&#47;&#47;number
var unit_net_sell_price_without_discount = BVK;&#47;&#47;number
var discount = LRAB;&#47;&#47;number (in%)
var unit_gross_sell_price_with_discount;
var total_gross_sell_price_with_discount = GSUM;&#47;&#47;number
var margin = (total_gross_sell_price_with_discount - total_buy_price) &#47; total_buy_price;&#47;&#47;number
var sign = TITEL;&#47;&#47;string - NO
var seller_acronym = VER;&#47;&#47;string
var provision = PROV;&#47;&#47;number

if ( sign == &apos;G&apos;) {
	quantity = -quantity;
	total_buy_price = -total_buy_price;
	total_gross_sell_price_with_discount = -total_gross_sell_price_with_discount;
}

if ( null != discount &amp;&amp; 0 != discount) {
	unit_gross_sell_price_with_discount = (100-discount)&#47;100*unit_gross_sell_price_without_discount;
}
else {
	unit_gross_sell_price_with_discount = unit_gross_sell_price_without_discount;
}</jsScript_script>
