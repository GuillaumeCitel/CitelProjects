        <jsScript_script>var ar_customer_key = SKARCustomer;&#47;&#47;number
var ar_customer_id = ID;&#47;&#47;string
var ar_customer_name = Name;&#47;&#47;string
var billing_address = Address1 + &quot;, &quot; + Address2+&quot;, &quot; + City + &quot;, &quot; + State;&#47;&#47;string
