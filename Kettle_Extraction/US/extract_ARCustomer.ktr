var area = Custom3;&#47;&#47;string
var area_name;&#47;&#47;string

switch (String(area)) {
	case &apos;1&apos;:
	case &apos;11&apos;:
		area_name = &apos;South (1 or 11)&apos;;
		break;
	case &apos;2&apos;:
