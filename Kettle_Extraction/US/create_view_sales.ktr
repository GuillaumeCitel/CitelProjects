    <sql>DROP VIEW IF EXISTS sales;
CREATE OR REPLACE VIEW sales AS
SELECT
	oe_lineitem.oe_lineitem_id,
	oe_lineitem.oe_lineitem_description,
	oe_lineitem.oe_lineitem_concat_description,
	ic_part.ic_part_key,
	ic_part.ic_part_id,
	ic_part.description,
	ic_part.concat_description,
	matching_part_and_group.part_group,
	oe_lineitem.shipped_qty,
	oe_lineitem.unit_price,
	oe_lineitem.unit_discount,
	oe_lineitem.extended_price,
	oe_lineitem.oe_lineitem_amount,
	CASE oe_lineitem.extended_price WHEN 0 THEN oe_lineitem.oe_lineitem_amount ELSE oe_lineitem.extended_price END as final_price,
	oe_lineitem.cumulative_cost,
	oe_lineitem.oe_lineitem_type,
	oe_lineitem.entity_type,
	ar_invoice.invoice_date,
	ar_invoice.ar_invoice_status,
	ar_invoice.status_description,
	ar_invoice.invoice_number,
	ar_customer.ar_customer_id,
	ar_customer.ar_customer_name,
	ar_customer.billing_state_or_country,
	ar_customer.country,
	ar_customer.person_nb,
	ar_customer.area,
	matching_person_number_and_acronym.person_acronym,
	ar_salesaccount.ar_salesaccount_description,
	gl_account.gl_account_description
from oe_lineitem
	left outer join oe_invoice on oe_lineitem.oe_entity_key = oe_invoice.oe_invoice_key
	left outer join ar_invoice on oe_invoice.ar_invoice_key=ar_invoice.ar_invoice_key
	left outer join ar_customer on ar_invoice.ar_customer_key=ar_customer.ar_customer_key
	left outer join ic_part on ic_part.ic_part_key=oe_lineitem.ic_part_key
	left outer join matching_part_and_group on matching_part_and_group.part_description = oe_lineitem.oe_lineitem_description
	left outer join matching_person_number_and_acronym on matching_person_number_and_acronym.person_number = ar_customer.person_nb
	left outer join ar_salesaccount on ar_salesaccount.ar_salesaccount_key = oe_lineitem.ar_salesaccount_key
	left outer join gl_account on gl_account.gl_account_number=ar_salesaccount.gl_account_number
where ar_invoice.status_description &lt;&gt; &apos;Voided&apos;
	and oe_lineitem.entity_type = &apos;I&apos;
	and ar_invoice.invoice_date is not null
	and (oe_lineitem.extended_price &lt;&gt; 0 or oe_lineitem.oe_lineitem_amount &lt;&gt; 0)</sql>
