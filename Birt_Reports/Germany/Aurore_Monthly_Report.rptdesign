            <method name="beforeOpen"><![CDATA[query_String = this.queryText;

list_of_excluded_codes = ["1000","1002-SCUH","1003"];

query_select = " select * ";
query_from = " from sales ";

month_n_plus_1 = parseInt(reportContext.getParameterValue("month"));
month_n_plus_1 += 1
query_where = query_String.substring(query_String.indexOf("where"));
query_where += " and bill_date >= " + "'" + reportContext.getParameterValue("year") + "-" + reportContext.getParameterValue("month") + "-01'"
if (month_n_plus_1 > 12){
	query_where += " and bill_date <  " + "'" + reportContext.getParameterValue("year") + "-12-31'"
}
else{
	query_where += " and bill_date <  " + "'" + reportContext.getParameterValue("year") + "-" + month_n_plus_1 + "-01'"
}
for (var i=0;i<list_of_excluded_codes.length;i++){
	query_where += " and product_gmbh_code <> " + "'" + list_of_excluded_codes[i] + "'";
}

this.queryText = query_select + query_from + query_where;]]></method>
